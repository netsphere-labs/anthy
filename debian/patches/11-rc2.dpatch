+{
+  char *buf = malloc(strlen(yomi) + strlen(word) + strlen(wt_name) + 20);
+  int rv;
+  if (!buf) {
+    return -1;
+  }
+  sprintf(buf, "%s %s*%d %s\n", yomi, wt_name, freq, word);
+  rv = anthy_textdict_insert_line(td, offset, buf);
+  free(buf);
+  return rv;
+}
